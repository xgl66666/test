# Select (enable / disable)

DDI_MMSDK_CTRL_DYNAMIC_SCALING = enable
DDI_MMSDK_CTRL_MVC = disable
DDI_MMSDK_CTRL_FRC = enable

DDI_MMSDK_DISPLAY_DUALXC = disable

DDI_MMSDK_AUDIO_MULTI = disable

DDI_MMSDK_DIVX = enable

# Set

# MM FRC mode select
# 0 - use DDI_MMSDK_CTRL_FRC to MM FRC
# 1 - Kano mode (P mode FRC by VDEC, I mode FRC by XC, HDR mode FRC by XC)
DDI_MMSDK_FRC_MODE = 0

# Start from 0
DDI_MMSDK_SUBTITLE_GOP = 2
